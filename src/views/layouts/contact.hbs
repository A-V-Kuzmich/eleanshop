<section class="contacts">
  <h2 class="contacts__heading">КОНТАКТЫ</h2>
  <div class="contacts__map-wrapper">
    <iframe
      src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2539.8326795462394!2d30.51503211561638!3d50.4628403794771!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x40d4ce41b95222ff%3A0xf723a2ac76932fd0!2z0JrQvtC90YLRgNCw0LrRgtC-0LLQsNGPINC_0LsuLCA4LCDQmtC40LXQsiwgMDQwNzA!5e0!3m2!1sru!2sua!4v1632770131462!5m2!1sru!2sua"
      width="669" height="694" style="border:0;" allowfullscreen="" loading="lazy">
    </iframe>
  </div>
  <div class="contacts__contacts-wrapper">
    <ul class="contacts__list">
      <li class="contacts__item"><a class="contacts__link"
          href='tel:{{phoneNumber}}'>ТЕЛ.: {{phoneNumberFormated}}</a></li>
      <li class="contacts__item"><a class="contacts__link" href='mailto:{{email}}'>EMAIL:
          {{email}}</a></li>
      <li class="contacts__item"><a class="contacts__link" href={{viberRef}}>VIBER:
          {{viberNumber}}</a></li>
    </ul>
    <ul class="contacts__list">
      <li class="contacts__item">{{entrepreneurName}}</li>
      <li class="contacts__item">{{entrepreneurInfo}}</li>
    </ul>
    <h3 class="contacts__subheading">АДРЕС И РЕЖИМ РАБОТЫ</h3>
    <ul class="contacts__list">
      <li class="contacts__item">{{visitingHours}}</li>
      <li class="contacts__item">АДРЕС: {{adress}}</li>
    </ul>
  </div>
  <div class="contacts__form">
    {{!-- тут форма примерки --}}
  </div>
</section>